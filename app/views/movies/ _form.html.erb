
    <%=form_with(model: movie) do |f| %>     
    <%= f.label :title %>
    <%= f.text_field :title %>
  
    <%= f.label :description %>
    <%= f.text_area :description %>
  
    <%= f.label :rating %>
    <%= f.text_field :rating %>
  
    <%= f.label :release_on %>
    <%= f.date_select :release_on %>
  
    <%= f.label :total_gross %>
    <%= f.number_field :total_gross %>
    <%= f.submit %>
    <% end %>
